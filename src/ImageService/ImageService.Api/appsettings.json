{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "azure_blob": {
    "endpoint": "https://<account>.blob.core.windows.net/",
    "account": "<account>",
    "key": "<key>",
    "container": "test"
  },

  "image_settings": {
    "MaxFileLength": "1200000",

    "RootDirectory": "intest/",

    "DirectoryTiny":   "tiny/",
    "DirectorySmall":  "small/",
    "DirectoryMedium": "medium/",
    "DirectoryBig":    "big/",
    "DirectoryLarge":  "large/"
  },

  "AllowedHosts": "*"
}
