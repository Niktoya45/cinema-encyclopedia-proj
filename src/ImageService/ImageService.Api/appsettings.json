{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "azure_blob": {
    "endpoint": "https://<account>.blob.core.windows.net/",
    "account": "<account>",
    "key": "<key>",
    "container": "test"
  },

  "image_settings": {
    "MaxFileLength": "1200000",

    "RootDirectory": "intest/",

    "Directory": {
      "None": "",
      "Tiny": "tiny/",
      "Small": "small/",
      "Medium": "medium/",
      "Big": "big/",
      "Large": "large/"
    },

    "Normalize": {
      "Tiny": "60",
      "Small": "120",
      "Medium": "220",
      "Big": "675",
      "Large": "1000"
    },

    "Breakpoints": {
      "Tiny": "0",
      "Small": "10800",
      "Medium": "37400",
      "Big": "303750",
      "Large": "840000"
    }

  },

  "AllowedHosts": "*"
}
